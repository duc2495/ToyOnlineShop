<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<!-- Configure NameSpace for action web -->
	<package name="Staff" namespace="/staff" extends="default-web">
		<action name="">
			<result>Staff.Login</result>
		</action>

		<action name="login">
			<result>Staff.Login</result>
		</action>

		<action name="dashboard">
			<result>Staff.Dashboard</result>
		</action>

		<action name="products">
			<result>Staff.Products</result>
		</action>

		<action name="product_edit">
			<result>Staff.ProductEdit</result>
		</action>

		<action name="orders">
			<result>Staff.Orders</result>
		</action>

		<action name="order_view">
			<result>Staff.OrderView</result>
		</action>

		<action name="customer">
			<result>Staff.Customer</result>
		</action>

		<action name="profile">
			<result>Staff.Profile</result>
		</action>
	</package>
</struts>